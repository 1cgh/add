	//Для Каждого КолонкаТз Из ТЗ.Колонки Цикл
	//	ИмяКолонки = КолонкаТз.Имя;
	//	
	//	
	//	ЗначТЗ = СтрокаТз[ИмяКолонки];
	//	ДлинаЗнач = СтрДлина(ЗначТЗ);
	//	
	//	СтрокаОшибок = СтрокаОшибок + "|" + ЗначТЗ;
